{
  "name": "monitor-server",
  "version": "0.1.0",
  "description": "Access Monitor Suite backend application server",
  "author": "Jo√£o Vicente",
  "license": "MIT",
  "scripts": {
    "prebuild": "rimraf dist",
    "typeorm": "node --require ts-node/register ./node_modules/typeorm/cli.js --config src/ormconfig.ts",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint '{src,apps,libs,test}/**/*.ts' --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test/govUser": "jest gov-user/gov-user.service.spec.ts",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:aStatement": "jest accessibility-statement/page-parser-test.spec.ts"
  },
  "dependencies": {
    "@nestjs/axios": "2.0.0",
    "@nestjs/common": "^9.3.9",
    "@nestjs/core": "^9.3.9",
    "@nestjs/jwt": "^10.0.2",
    "@nestjs/mapped-types": "1.2.2",
    "@nestjs/passport": "^9.0.3",
    "@nestjs/platform-express": "^9.3.9",
    "@nestjs/schedule": "^2.2.0",
    "@nestjs/serve-static": "^3.0.1",
    "@nestjs/swagger": "^6.2.1",
    "@nestjs/typeorm": "9.0.1",
    "@qualweb/core": "^0.8.4",
    "axios": "^1.3.4",
    "bcrypt": "5.1.0",
    "body-parser": "^1.20.2",
    "class-validator": "0.14.1",
    "compression": "^1.7.4",
    "css-select": "^5.1.0",
    "file-saver": "^2.0.5",
    "helmet": "^6.0.1",
    "htmlparser2": "^8.0.1",
    "ip-range-check": "^0.2.0",
    "lodash.clone": "^4.5.0",
    "lodash.clonedeep": "^4.5.0",
    "lodash.orderby": "^4.6.0",
    "mysql": "^2.18.1",
    "mysqldump": "^3.2.0",
    "nest-winston": "^1.9.1",
    "nestjs-rate-limiter": "^3.1.0",
    "object-hash": "^3.0.0",
    "passport": "^0.6.0",
    "passport-jwt": "4.0.0",
    "passport-local": "^1.0.0",
    "puppeteer": "^22.12.1",
    "puppeteer-extra": "^3.3.6",
    "puppeteer-extra-plugin-stealth": "^2.11.2",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^4.3.1",
    "rxjs": "^7.8.0",
    "sqlstring": "^2.3.3",
    "svg-builder": "^2.0.0",
    "winston": "^3.8.2",
    "winston-daily-rotate-file": "^4.7.1"
  },
  "devDependencies": {
    "@nestjs/cli": "^9.2.0",
    "@nestjs/schematics": "^9.0.4",
    "@nestjs/testing": "^9.3.9",
    "@qualweb/act-rules": "0.7.5",
    "@qualweb/best-practices": "0.7.7",
    "@qualweb/counter": "0.3.2",
    "@qualweb/qw-page": "0.3.2",
    "@qualweb/util": "0.6.4",
    "@qualweb/wcag-techniques": "0.4.2",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.4.0",
    "@types/lodash.clone": "^4.5.7",
    "@types/lodash.orderby": "^4.6.7",
    "@types/mocha": "^10.0.1",
    "@types/node": "^16.11.7",
    "@types/passport-jwt": "^3.0.8",
    "@types/passport-local": "^1.0.35",
    "@types/sqlstring": "^2.3.0",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^5.54.1",
    "@typescript-eslint/parser": "^5.54.1",
    "eslint": "^8.35.0",
    "eslint-config-prettier": "^8.7.0",
    "eslint-plugin-import": "^2.27.5",
    "jest": "^29.5.0",
    "prettier": "^2.8.4",
    "supertest": "^6.3.3",
    "ts-jest": "^29.0.5",
    "ts-loader": "^9.4.2",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.1.2",
    "typescript": "<4.8"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "../coverage",
    "testEnvironment": "node",
    "moduleNameMapper": {
      "^src/(.*)$": "<rootDir>/$1"
    }
  }
}
